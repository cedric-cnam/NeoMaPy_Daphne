<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project NeoMaPyGraph">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <!--define folder properties-->
    <property name="dir.buildfile" value="."/>
    <property name="dir.workspace" value="/Users/ntravers/eclipse-workspace"/>
    <property name="dir.jarfile" value="${dir.buildfile}/neo4jDocker_folder"/>
    <target name="create_run_jar">
        <jar destfile="${dir.jarfile}/graphModeling.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="NeoMaPy"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="${dir.buildfile}/bin"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/json-simple-1.1.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/neo4j-java-driver-4.4.9.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/neo4j-java-driver-slim-4.4.9.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/micrometer-core-1.9.3.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/reactive-streams-1.0.4.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/slf4j-api-2.0.0.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="${dir.buildfile}/lib/svm-22.2.0.jar"/>
        </jar>
    </target>
</project>
